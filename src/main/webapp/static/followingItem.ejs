<div id = "followingItem_<%= uid %>" class = "span-14 item-row">
    <div class = "span-2">
        <a href = "/user?uid=<%= uid %>">
            <img src = "/static/images/default-profile-picture.gif" height = 50 width = 60/>
        </a>
    </div>
    <div class = "span-9">
        <h3><a href= '/user?uid=<%= uid %>'><%= name %></a> </h2>
        <h6><%= email %></h5>
    </div>
    <div class = "span-2 last follow-button">
        <script type = "text/javascript">
            alert(${uid} + " " + ${user});
        </script>
        <% if( !user.equals(uid) ) {
                if(status == 1) { %>
                    <input type = "button" value = "unfollow" onclick = "unfollow(<%= uid %>);" />
            <%  }
                else { %>
                    <input type = "button" value = "follow" onclick = "follow(<%= uid %>);" />
            <% }
        } %>
    </div>
</div>